<h1><%= @course.title %></h1>
<h3>Curator: <%= @course.owner.username %></h3>

<%= form_for @course do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.submit %>
<% end %>
