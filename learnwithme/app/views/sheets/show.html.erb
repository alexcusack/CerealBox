<div class="row">
  <div class="col-sm-6">
    <h1><%= @sheet.title %></h1>
    <p><%= @sheet.description %></p>
  </div>
  <div class="col-sm-6">
    <div class="row">
      <div class="col-sm-12">
        <img class="img-responsive" src="<%= @sheet.image %>"/>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <h2>Map</h2>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <%= @sheet.video %>
      </div>
    </div>
  </div>
</div>


<%= link_to 'Edit', edit_sheet_path(@sheet) %> |
<%= link_to 'Back', sheets_path %>

<script src="http://maps.googleapis.com/maps/api/js"></script>
<%= javascript_include_tag 'map', 'data-turbolinks-track' => true %>
